<Window x:Class="GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#607D8B">
        <Border BorderBrush="White" BorderThickness="1" HorizontalAlignment="Center" Height="250" Margin="0,50,0,0" VerticalAlignment="Top" Width="750" Background="#000000">
            <ScrollViewer Foreground="Black" CanContentScroll="True">
                <TextBlock x:Name="Console" TextWrapping="Wrap" Text=" TextBlock" Margin="10,10,0,0" Foreground="White" Background="#000000"/>
            </ScrollViewer>
        </Border>
        <Button x:Name="Enter" Content="Enter" HorizontalAlignment="Left" Margin="700,350,0,0" VerticalAlignment="Top" Width="75" Height="30" Click="EnterClick" Background="#000000" Foreground="White" IsDefault="True"/>
        <TextBox x:Name="commandBox" HorizontalAlignment="Left" Height="31" Margin="25,350,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="660" TextChanged="TextChanged" Background="#000000" Foreground="White"/>
    </Grid>
</Window>
